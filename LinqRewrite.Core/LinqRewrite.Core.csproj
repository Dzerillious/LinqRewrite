<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <PackageVersion>1.0.10</PackageVersion>
        <Title>LinqRewrite.Core</Title>
        <Authors>Daniel Šerý</Authors>
        <ApplicationIcon>icon.ico</ApplicationIcon>
        <PackageIcon>icon.png</PackageIcon>
        <Description>Core of LinqRewrite NuGet for rewriting of Linq queries</Description>
        <PackageReleaseNotes>Simplified SimpleList</PackageReleaseNotes>
        <LangVersion>Latest</LangVersion>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <TargetFrameworks>net40;net45;net462;netcoreapp2.1</TargetFrameworks>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="icon.ico" />
      <Resource Include="icon.ico" />
    </ItemGroup>
    <ItemGroup>
        <None Include="icon.png" Pack="true" PackagePath=""/>
    </ItemGroup>
</Project>
