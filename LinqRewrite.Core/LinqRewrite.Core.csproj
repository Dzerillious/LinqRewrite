        <PackageReleaseNotes>Fixed SimpleLinq unknown size operators, using inner ArrayCopy</PackageReleaseNotes>
        <LangVersion>Latest</LangVersion>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <TargetFrameworks>net40;net45;net462;netcoreapp2.1</TargetFrameworks>
        <RepositoryUrl>https://gitlab.nesad.fit.vutbr.cz/xseryd00/roslyn-linq-rewrite/</RepositoryUrl>
        <PackageTags>performance; LINQ; rewrite; algorithms</PackageTags>
    </PropertyGroup>
</Project>
